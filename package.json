{
  "name": "auto-pinner",
  "version": "1.0.0",
  "description": "fiverr to pinterest automation",
  "main": "main.js",
  "scripts": {
    "start": "electron ."
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^10.1.1"
  },
  "dependencies": {
    "base-64": "^1.0.0",
    "image-downloader": "^4.0.1",
    "lowdb": "^1.0.0",
    "node-fetch": "^2.6.1",
    "puppeteer-core": "^5.2.1",
    "puppeteer-in-electron": "^3.0.3",
    "request-image-size": "^2.2.0"
  },
  "build": {
    "appId": "auto-pinner",
    "win": {
      "target": "portable",
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "zip"
      ],
      "icon": "build/icon.png",
      "category": "Internet"
    }
  }
}
